# Arquivo de configuração para o pacote MajorTank
# Define as seguinter variáveis:
# MAJORTANK_INCLUDE_DIRS    - diretórios de cabeçalhos para MajorTank
# MAJORTANK_LIBRARIES       - dependências
# MAJORTANK_EXECUTABLE      - o executável

get_filename_component (MAJORTANK_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set (MAJORTANK_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")

#  dependências (contém definição para bibliotecas importadas)
if (NOT TARGET conf AND NOT MajorTank_BINARY_DIR)
    include ("${MAJORTANK_CMAKE_DIR}/MajorTankTargets.cmake")
endif()

# bibliotecas importadas criadas por MajorTankTargets.cmake
set (MAJORTANK_LIBRARIES conf)
set (MAJORTANK_EXECUTABLE game)
